    "@angular/forms": "^8.0.3",
    "@angular/platform-browser": "^8.0.3",
    "core-js": "^3.1.4",
    "lodash.clonedeep": "^4.5.0",
    "rxjs": "^6.5.2",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^8.0.3",
    "@angular-devkit/build-angular": "~0.800.6",
    "@angular-devkit/build-ng-packagr": "^0.800.6",
    "ng-packagr": "^5.3.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.10.0",
    "typescript": "3.4.5",
    "@angular/cli": "^8.0.6",
    "@angular/language-service": "^8.0.3",
