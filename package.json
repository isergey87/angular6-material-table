    "@types/node": "~12.0.10",
    "codelyzer": "~5.1.0",
    "rollup": "^1.16.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "uglify-js": "^3.6.0"
  },
  "repository": {
