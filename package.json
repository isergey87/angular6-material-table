{
  "version": "0.3.0",
  "name": "angular6-material-table",
  "description": "Angular 6 table based on angular4-material-table.",
  "keywords": [
    "angular",
    "angular6",
    "table",
    "inline validation",
    "angular material table"
  ],
  "author": "isergey",
  "license": "MIT",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/angular6-material-table.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/angular6-material-table.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^6.0.2",
    "@angular/common": "^6.0.1",
    "@angular/core": "^6.0.1",
    "@angular/forms": "^6.0.1",
    "core-js": "^2.5.6",
    "lodash.clonedeep": "^4.5.0",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "^6.0.1",
    "@angular/compiler": "^6.0.1",
    "@angular/compiler-cli": "^6.0.1",
    "@angular/language-service": "^6.0.1",
    "@types/jasmine": "2.8.7",
    "@types/node": "~10.0.3",
    "codelyzer": "~4.3.0",
    "rollup": "^0.59.0",
    "ts-node": "~6.0.3",
    "tslint": "~5.10.0",    
    "uglify-js": "^3.3.25"    
