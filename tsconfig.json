{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "stripInternal": true,
    "experimentalDecorators": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "module": "es2015",
    "moduleResolution": "node",
    "paths": {
      "@angular/core": [
        "node_modules/@angular/core"
      ],
      "rxjs/*": [
        "node_modules/rxjs/*"
      ],
      "angular6-material-table": [
        "dist/angular6-material-table"
      ],
      "angular6-material-table/*": [
        "dist/angular6-material-table/*"
      ]
    },
    "rootDir": ".",
    "outDir": "dist",
    "sourceMap": true,
    "inlineSources": true,
    "target": "es5",
    "skipLibCheck": true,
    "lib": [
      "es2015",
      "dom"
    ]
  },
  "files": [
    "index.ts"
  ],
  "angularCompilerOptions": {
    "strictMetadataEmit": true,
    "skipTemplateCodegen": true,
    "flatModuleOutFile": "angular6-material-table.js",
    "flatModuleId": "angular6-material-table",
    "annotateForClosureCompiler": true
  }
}
